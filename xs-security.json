{
  "xsappname": "orders",
  "tenant-mode": "dedicated",
  "scopes": [
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Admin"
    },
    {
      "name": "$XSAPPNAME.Customer",
      "description": "Customer"
    }
  ],
  "attributes": [],
  "role-templates": [
    {
      "name": "Admin",
      "description": "Can Perform both READ and WRITE operations",
      "scope-references": [
        "$XSAPPNAME.Admin"
      ],
      "attribute-references": []
    },
    {
      "name": "Customer",
      "description": "Can Perform only READ operations",
      "scope-references": [
        "$XSAPPNAME.Customer"
      ],
      "attribute-references": []
    }
  ],
  "authorities": [
    "$ACCEPT_GRANTED_AUTHORITIES"
  ],
  "oauth2-configuration": {
    "token-validity": 9000,
    "redirect-uris": [
      "https://*.cfapps.us10-001.hana.ondemand.com/login/callback"
    ]
  },
  "xsenableasyncservice": "true"
}
